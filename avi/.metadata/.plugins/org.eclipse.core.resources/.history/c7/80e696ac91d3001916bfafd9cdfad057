package com.avi.avi.model;

import org.springframework.stereotype.Component;

import com.vmware.o11n.plugin.sdk.annotation.*;
import org.json.*;
import org.json.simple.JSONValue;

@Component
public class virtualServiceObjectService  {
	
	private String key;
	private String value;
//	private JSONObject jsonData;
	
	public virtualServiceObjectService() {
    }
		
	public JSONArray get() {
		JSONArray data = new JSONArray();
		data.put(0, key);
		data.put(1, value);
		return data;
	}
	
//	public JSONObject json() {
////		String obj = JSONValue.toJSONString(json);
//		return jsonData;
//	}
		
	public String set(String key, String value) {
		this.key = key;
		this.value = value;
		return "in set function "+key+" >> "+value;
	}   
}

