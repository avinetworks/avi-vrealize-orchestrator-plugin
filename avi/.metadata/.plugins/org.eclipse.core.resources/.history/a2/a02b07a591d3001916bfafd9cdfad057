package com.avi.avi.model;

import org.springframework.stereotype.Component;

import com.vmware.o11n.plugin.sdk.annotation.*;
import org.json.*;
import org.json.simple.JSONValue;


@Component
public class poolObjectService  {
	
	private String key;
	private String value;
	public pool p1;
//	private JSONObject jsonData;
	
	public poolObjectService() {		
    }
		
	public JSONObject get() {
		JSONObject data = new JSONObject();
		data.put(key, value);
//		data.put(1, value);
		return data;
	}
	
//	public JSONObject json() {
////		String obj = JSONValue.toJSONString(json);
//		return jsonData;
//	}
		
	public String set(String key, String value) {
		this.key = key;
		this.value = value;
		return "in set function "+key+" >> "+value;
	}   
}



